reqpkg <- c("dplyr", "tidyverse", "tidyr", "stringr", "ggplot2", "here", "WDI", "wbstats", "DataExplorer", "ggridges", "gganimate", "egg", "ggtext", "tweenr", "animation", "grid", "lubridate")
sapply(reqpkg, function(pkgi) {
if (!pkgi %in% installed.packages()) {
install.packages(pkgi, repos = "http://cran.us.r-project.org")
}
library(pkgi, character.only = TRUE)
})
# inspect the working directory
here()
#### Call API to get WDI data ####
WDIsearch(string = "sanitation")
sanitation <- WDI(country = "all", indicator = "SH.STA.BASS.ZS", start = 1960,
end = NULL, extra = TRUE, cache = NULL)
